from google-drive-concourse-resource-common import getServiceInstance
from google-drive-concourse-resource-common import listFilesinFolder

"""Lists a version of a file from a google drive folder"""
fID='0BzoFwqMR_Vv4ZzZiSWJqcmozNzQ'

def main():
  drive_service = getServiceInstance()
  listFilesinFolder(drive_service,fID)

if __name__ = '__main__':
  main()
