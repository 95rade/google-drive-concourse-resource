from google-drive-concourse-resource import getServiceInstance(user,keyFile)
from google-drive-concourse-resource import listFilesinFolder(service,folderID)

"""Lists a version of a file from a google drive folder"""
fID='0BzoFwqMR_Vv4ZzZiSWJqcmozNzQ'

def main():
  drive_service = getServiceInstance()
  listFilesinFolder(drive_service,fID)

if __name__ = '__main__':
  main()
